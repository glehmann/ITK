itk_wrap_class("itk::ComponentTreeAttributeToImageFilter" POINTER)
  foreach(t ${WRAP_ITK_SCALAR})
    foreach(d ${WRAP_ITK_DIMS})
      itk_wrap_template("${ITKM_CT${t}${d}UL}${ITKM_IUL${d}}" "${ITKT_CT${t}${d}UL}, ${ITKT_IUL${d}}")
      foreach(t2 ${WRAP_ITK_REAL})
        itk_wrap_template("${ITKM_CT${t}${d}D}${ITKM_I${t2}${d}}" "${ITKT_CT${t}${d}D}, ${ITKT_I${t2}${d}}")
      endforeach(t2)
    endforeach(d)
  endforeach(t)
itk_end_wrap_class()
