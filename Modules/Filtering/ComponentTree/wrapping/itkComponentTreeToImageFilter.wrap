itk_wrap_class("itk::ComponentTreeToImageFilter" POINTER)
  foreach(t ${WRAP_ITK_SCALAR})
    foreach(d ${WRAP_ITK_DIMS})
      itk_wrap_template("${ITKM_CT${t}${d}UL}${ITKM_I${t}${d}}" "${ITKT_CT${t}${d}UL}, ${ITKT_I${t}${d}}")
      itk_wrap_template("${ITKM_CT${t}${d}D}${ITKM_I${t}${d}}" "${ITKT_CT${t}${d}D}, ${ITKT_I${t}${d}}")
    endforeach(d)
  endforeach(t)
itk_end_wrap_class()
