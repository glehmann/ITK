itk_wrap_class("itk::ComponentTreeLeavesToLabelImageFilter" POINTER)
  foreach(t1 ${WRAP_ITK_SCALAR})
    foreach(t2 ${WRAP_ITK_USIGN_INT})
      foreach(d ${WRAP_ITK_DIMS})
        itk_wrap_template("${ITKM_CT${t1}${d}UL}${ITKM_I${t2}${d}}" "${ITKT_CT${t1}${d}UL}, ${ITKT_I${t2}${d}}")
        itk_wrap_template("${ITKM_CT${t1}${d}D}${ITKM_I${t2}${d}}" "${ITKT_CT${t1}${d}D}, ${ITKT_I${t2}${d}}")
      endforeach(d)
    endforeach(t2)
  endforeach(t1)
itk_end_wrap_class()
